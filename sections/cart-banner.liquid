<div>
  <div class="cartBanner">
    <a href="{{ section.settings.guestBannerLink }}"><img src='{{ section.settings.guestBanner | img_url:'1920x'}}'></a>
    <a href="{{ section.settings.bubbleBannerLink }}"><img src='{{ section.settings.companyBanner | img_url:'1920x'}}'></a>
  </div>
</div>
{% schema %}
  {
    "name": "Cart Banner",
    "settings": [
      {
        "type": "image_picker",
        "id":"guestBanner",
        "label": "Guest User Banner"
      },
      {
        "type": "url",
        "id":"guestBannerLink",
        "label": "Guest Banner Link"
      },
      {
        "type": "image_picker",
        "id":"companyBanner",
        "label": "Bubble User Banner"
      },
      {
        "type": "url",
        "id":"bubbleBannerLink",
        "label": "Bubble Banner Link"
      }
    ],
    "presets": [
      {
        "name": "Cart Banner",
        "category":"section"
      }
    ]
  }
{% endschema %}