async function getVariantMetafieldValue(variantId, namespace, key) {
    // Replace with your Shopify store domain and access token
    const shopifyDomain = 'ickle-bubba-sandbox.myshopify.com';
    const adminAccessToken = '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';// Replace with your actual token

    // JSON formatted query
    const query = {
        query: `
        {
            productVariant(id: "gid://shopify/ProductVariant/${variantId}") {
                metafield(namespace: "${namespace}", key: "${key}") {
                    value
                }
            }
        }`
    };

    try {
        const response = await fetch(`https://${shopifyDomain}/admin/api/2024-07/graphql.json`, {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
                'X-Shopify-Access-Token': adminAccessToken,
            },
            body: JSON.stringify(query)
        });

        const result = await response.json();

        if (response.ok && result.data.productVariant.metafield) {
            return result.data.productVariant.metafield.value;
        } else {
            console.error('Error fetching metafield value:', result.errors || 'Metafield not found');
            return null;
        }
    } catch (error) {
        console.error('Error fetching metafield value:', error);
        return null;
    }
}

// Function to monitor attribute changes
function observeAttributeChange(element, attributeName, callback) {
    // Create a new MutationObserver instance
    const observer = new MutationObserver((mutationsList) => {
        for (const mutation of mutationsList) {
            // Check if the attribute we're interested in has changed
            if (mutation.type === 'attributes' && mutation.attributeName === attributeName) {
                const newValue = element.getAttribute(attributeName);
                callback(newValue);
            }
        }
    });

    // Start observing the element for attribute changes
    observer.observe(element, {
        attributes: true // Observe changes to attributes
    });
}

